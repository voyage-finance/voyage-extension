{
  "name": "Voyage Extension",
  "description": "Enables playing/buy through your Voyage vault!",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": { "service_worker": "background.bundle.js" },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["contentScript.bundle.js"]
  }],
  "icons": { "128": "icon.png" }
}
